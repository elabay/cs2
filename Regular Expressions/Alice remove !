import re
def remove(text):
    for line in text:
        for char in line:
            if re.search('$!', char):
                del char
    return text










def main():
    #file = open("test Text file_Guillotine", "r")
    file = open("Alice in Wonderland", "r")
    text = file.read()
    remove('hello world! My name is elly!')
main()
